    "    mask = df['累计播放量'].str.endswith('千万').fillna(False)\n",
    "    df.loc[mask, '累计播放量'] = pd.to_numeric(df.loc[mask, '累计播放量'].str.replace('千万',''))/10\n",
    "    mask1 = df['累计播放量'].str.endswith('万').fillna(False)\n",
    "    df.loc[mask1, '累计播放量'] = pd.to_numeric(df.loc[mask1, '累计播放量'].str.replace('万',''))/10000\n",
    "    return"
   ]
